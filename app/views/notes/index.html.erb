<h1>Listing notes</h1>

<table>
  <thead>
    <tr>
      <th>Project ID</th>
      <th>User</th>
      <th>Name</th>
      <th>Start</th>
      <th>Due</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><%= note.project_id %></td>
        <td><%= note.user_id %></td>
        <td><%= note.name %></td>
        <td><%= note.start %></td>
        <td><%= note.due %></td>
        <td><%= link_to 'Show', note %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
        <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_project_note_path(@project) %>
